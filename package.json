{
  "name": "sgmea-tcc-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run clean && ng serve --host 0.0.0.0 --proxy-config proxy.config-server.js --configuration=development",
    "start:prod": "npm run clean && ng serve --host 0.0.0.0 --proxy-config proxy.config-prod.js --configuration=production",
    "clean": "rm -rf package-lock.json .dist/ dist/",
    "updatepackage": "ncu && ncu -u",
    "reinstall": "rm -rf node_modules/ && npm install --legacy-peer-deps",
    "install": "npm install --legacy-peer-deps",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:dev": "ng build --configuration=development",
    "watch": "ng build --watch --configuration=development",
    "test": "ng test",
    "serve:ssr:sgmea-tcc-frontend": "node dist/sgmea-tcc-frontend/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular-package/sass": "^0.19.0-beta",
    "@angular/animations": "17.3.8",
    "@angular/cdk": "17.0.1",
    "@angular/common": "17.3.8",
    "@angular/compiler": "17.3.8",
    "@angular/core": "17.3.8",
    "@angular/forms": "17.3.8",
    "@angular/material": "17.0.1",
    "@angular/material-luxon-adapter": "17.0.1",
    "@angular/platform-browser": "17.3.8",
    "@angular/platform-browser-dynamic": "17.3.8",
    "@angular/platform-server": "17.3.8",
    "@angular/router": "17.3.8",
    "@angular/ssr": "17.3.8",
    "@fortawesome/fontawesome-free": "6.6.0",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.4",
    "apexcharts": "^3.44.0",
    "chart.js": "^4.4.1",
    "circular-dependency-plugin": "^5.2.2",
    "class-transformer": "0.2.3",
    "core-js": "^3.36.1",
    "express": "4.18.2",
    "moment": "^2.27.0",
    "ng-apexcharts": "^1.8.0",
    "ng2-logger": "16.444.15",
    "ng2-pdf-viewer": "^10.3.1",
    "ngx-mask": "^18.0.0",
    "print-js": "1.6.0",
    "rxjs": "7.8.1",
    "tailwind-scrollbar": "1.3.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.8",
    "@angular/cli": "17.3.8",
    "@angular/compiler-cli": "17.3.8",
    "@types/express": "4.17.17",
    "@types/jasmine": "~3.10.0",
    "jasmine-core": "~3.10.0",
    "jquery": "^3.7.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.12",
    "tailwind-scrollbar": "1.3.1",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2"
  }
